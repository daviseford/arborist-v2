{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": false,
        "ban-types": false,
        "max-classes-per-file": false,
        "no-console": false,
        "no-var-requires": false,
        "only-arrow-functions": true,
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "variable-name": false
    },
    "rulesDirectory": []
}